{
  "name": "bce-client",
  "productName": "百度云",
  "version": "0.3.1-beta",
  "description": "百度云-客户端",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "pretest": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty app",
    "lint-fix": "npm run lint -- --fix",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "compile": "npm run build-main && npm run build-renderer",
    "preview": "cross-env NODE_ENV=production electron ./static/",
    "start": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./app/main",
    "server": "cross-env NODE_ENV=development node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack.config.development.js",
    "dev": "cross-env START_HOT=1 npm run server",
    "build": "npm run compile && build --publish never",
    "build:win": "npm run compile && build --win --ia32 --publish never",
    "build:osx": "npm run compile && build --mac --publish never",
    "publish:bos": "node -r babel-register ./publish/bosPublisher.js",
    "publish:github": "node -r babel-register ./publish/githubPublisher.js"
  },
  "build": {
    "appId": "com.baidu.bce",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "dmg": {
      "contents": [
        {
          "x": 400,
          "y": 225,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 150,
          "y": 230
        }
      ]
    },
    "files": [
      "!*.{eot,ttf}"
    ],
    "directories": {
      "buildResources": "build",
      "output": "dist",
      "app": "static"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mudio/bce-client"
  },
  "author": {
    "name": "木休大人",
    "email": "job.zhanghao@gmail.com",
    "url": "https://github.com/mudio"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mudio/bce-client/issues"
  },
  "keywords": [
    "bce",
    "bos"
  ],
  "homepage": "https://github.com/mudio/bce-client#readme",
  "devDependencies": {
    "ajv": "^5.1.1",
    "asar": "^0.13.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-tcomb": "^0.3.25",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.1",
    "concurrently": "^3.4.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "css-modules-require-hook": "^4.0.6",
    "del": "^2.2.2",
    "devtron": "^1.4.0",
    "electron": "^1.6.10",
    "electron-builder": "^18.6.2",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "2.1.0",
    "fbjs-scripts": "^0.8.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "js-yaml": "^3.8.4",
    "jsdom": "^11.0.0",
    "json-loader": "^0.5.4",
    "mime": "^1.3.6",
    "prop-types": "^15.5.10",
    "react-hot-loader": "3.0.0-beta.7",
    "redux-logger": "^3.0.6",
    "source-map-support": "^0.4.15",
    "style-loader": "^0.18.1",
    "tcomb": "^3.2.20",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "antd": "^2.10.4",
    "async": "^2.4.0",
    "bce-sdk-js": "^0.2.7",
    "classnames": "^2.2.5",
    "configstore": "^3.1.0",
    "electron-log": "^2.2.6",
    "electron-updater": "^2.1.1",
    "font-awesome": "^4.7.0",
    "fs-walk": "0.0.1",
    "history": "^4.6.1",
    "keytar": "^4.0.3",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "q": "^1.5.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.0.1"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
