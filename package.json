    "spectron": "^3.6.0",
    "style-loader": "^0.13.1",
    "tcomb": "^3.2.16",
    "url-loader": "^0.5.7",
    "webpack": "^1.9.11",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^3.0.0",
    "webpack-validator": "^2.3.0",
    "worker-loader": "^0.8.0"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "async": "^2.1.4",
    "bce-sdk-js": "^0.2.3",
    "classnames": "^2.2.5",
    "configstore": "^3.0.0",
    "css-modules-require-hook": "^4.0.5",
    "electron-debug": "^1.1.0",
    "electron-log": "^1.3.0",
    "electron-updater": "^1.6.0",
    "font-awesome": "^4.7.0",
    "fs-walk": "0.0.1",
    "keytar": "^3.0.2",
    "lodash": "^4.17.2",
    "mkdirp": "^0.5.1",
    "q": "^1.4.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.0.1"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
