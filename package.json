{
  "name": "bce-bos-client",
  "productName": "BOS",
  "version": "0.2.4-beta",
  "description": "百度云-对象存储BOS-客户端",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
    "pretest": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "test-e2e": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require ./test/setup.js ./test/e2e.js",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty app test",
    "lint-fix": "npm run lint -- --fix",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "compile": "npm run build-main && npm run build-renderer",
    "preview": "cross-env NODE_ENV=production electron ./static/",
    "start": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./app/main",
    "server": "cross-env NODE_ENV=development node -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack.config.development.js",
    "dev": "cross-env START_HOT=1 npm run server",
    "build": "npm run compile && build --publish never",
    "build:win": "npm run compile && build --win --x64",
    "build:osx": "npm run compile && build --mac --publish never",
    "publish:bos": "node -r babel-register ./publish/bosPublisher.js",
    "publish:github": "node -r babel-register ./publish/githubPublisher.js"
  },
  "build": {
    "appId": "com.baidu.bce",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "dmg": {
      "contents": [
        {
          "x": 400,
          "y": 225,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 150,
          "y": 230
        }
      ]
    },
    "files": [
      "!*.{map,eot,ttf,woff2}"
    ],
    "directories": {
      "buildResources": "build",
      "output": "dist",
      "app": "static"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mudio/bce-client"
  },
  "author": {
    "name": "木休大人",
    "email": "job.zhanghao@gmail.com",
    "url": "https://github.com/mudio"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mudio/bce-client/issues"
  },
  "keywords": [
    "bce",
    "bos"
  ],
  "homepage": "https://github.com/mudio/bce-client#readme",
  "devDependencies": {
    "asar": "^0.13.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-tcomb": "^0.3.24",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-es2015-classes": "^6.22.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "babili-webpack-plugin": "^0.0.11",
    "concurrently": "^3.2.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.27.3",
    "css-modules-require-hook": "^4.0.5",
    "del": "^2.2.2",
    "devtron": "^1.4.0",
    "electron": "^1.6.2",
    "electron-builder": "^15.6.1",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.1.0",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "2.1.0",
    "fbjs-scripts": "^0.7.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "js-yaml": "^3.8.2",
    "jsdom": "^9.10.0",
    "json-loader": "^0.5.4",
    "mime": "^1.3.4",
    "minimist": "^1.2.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-logger": "^2.8.1",
    "sinon": "^2.0.0",
    "spectron": "^3.6.0",
    "style-loader": "^0.14.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "async": "^2.1.5",
    "bce-sdk-js": "^0.2.5",
    "classnames": "^2.2.5",
    "configstore": "^3.0.0",
    "electron-log": "^2.0.0",
    "electron-updater": "^1.10.3",
    "font-awesome": "^4.7.0",
    "fs-walk": "0.0.1",
    "keytar": "^3.0.2",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "q": "^1.4.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "source-map-support": "^0.4.13",
    "uuid": "^3.0.1"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
